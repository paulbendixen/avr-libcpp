#ifndef STD_UTILITY
#define STD_UTILITY
namespace std
{
	template < class T1, class T2 >
	struct pair{
		using first_type = T1;
		using second_type = T2;
		T1 first;
		T2 second;
		pair( const pair& ) = default;
		pair( pair&& ) = default;
		consexpr pair() = default;
	};
}

// vim: se ft=cpp:
